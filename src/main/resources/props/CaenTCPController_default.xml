<?xml version="1.0" encoding="UTF-8"?>

<!-- This controller allows to connect to a remote caen reader through its 
	host computer assuming that the corresponding service was installed. CaenProxyService 
	must be installed for this to work -->

<properties>

	<!-- 'http://localhost:8888/caenrfid-proxy' is the default configuration 
		for the service published by the remote reader proxy -->
	<readerServiceEndpoint>http://localhost:8888/caenrfid-proxy</readerServiceEndpoint>

	<!-- Configuration file with the transponder model configuration -->
	<epcTransponderModelsConfig>/props/EPCTransponderModels_default.xml</epcTransponderModelsConfig>

	<!-- The IdTypes configuration file -->
	<idTypesConfig>/props/IDTypes_default.xml</idTypesConfig>

</properties>